programa {
	funcao inicio() {
	    //declaração de variaveis
	    real euro, compra, pontos
	    
	    //entrada de dados
	    escreva("Digite o valor do Euro..: ")
	    leia(euro)
	    escreva("Digite o valor da compra..: ")
	    leia(compra)
	    
	    //processamento
	    pontos = compra
	    
	    //saida de dados
	    escreva("Quantidade de pontos ganhos..: ", pontos)
	    
	}
}
